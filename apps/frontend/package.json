{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "fe:dev": "bunx --bun vite --port=7102 --host",
    "fe:build": "bunx --bun vite build",
    "fe:start": "bun ./dist/index.html --port=7102",
    "fe:preview": "bunx --bun vite preview --port=7102",
    "fe:lint": "bunx --bun biome lint",
    "fe:check": "bunx --bun biome check --write",
    "fe:fmt": "bunx --bun biome check --write --linter-enabled=false",
    "fe:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@writtte-internal/translations": "workspace:*",
    "@writtte-internal/validate": "workspace:*",
    "@writtte-internal/local-storage": "workspace:*",
    "@writtte-internal/store": "workspace:*",
    "@writtte-internal/indexed-db": "workspace:*",
    "@writtte-internal/clipboard": "workspace:*",
    "@writtte-editor/editor": "workspace:*",
    "@writtte-editor/export": "workspace:*",
    "@fontsource-variable/source-sans-3": "5.2.9",
    "@fontsource-variable/jetbrains-mono": "5.2.8",
    "@fontsource-variable/playfair": "5.2.8",
    "@fontsource/special-elite": "5.2.8",
    "@sentry/browser": "10.32.1",
    "chart.js": "4.5.1",
    "micromark": "4.0.2"
  },
  "devDependencies": {
    "@types/bun": "1.3.2",
    "vite": "npm:rolldown-vite@7.2.2"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.2"
  }
}
